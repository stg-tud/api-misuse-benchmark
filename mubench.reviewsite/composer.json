{
  "name": "MuBench/ReviewSite",
  "description": "The MuBench review site component.",
  "require": {
    "monolog/monolog": "1.21.*",
    "slim/slim": "3.5.0",
    "slim/php-view": "2.1.0",
    "tuupola/slim-basic-auth": "^2.2",
    "usmanhalalit/pixie": "2.0.2",
    "erusev/parsedown": "^1.6",
    "illuminate/database": "v5.5.2",
    "illuminate/events": "v5.5.17"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "facebook/webdriver": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "MuBench\\ReviewSite\\": "src",
      "MuBench\\ReviewSite\\Setup\\": "setup"
    },
    "files": [
      "src/route_utils.php",
      "src/csv_utils.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MuBench\\ReviewSite\\Tests\\": "tests"
    }
  }
}
